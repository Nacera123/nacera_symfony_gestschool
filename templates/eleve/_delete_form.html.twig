<form method="post" action="{{ path('eleve_delete', {'id': eleve.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ eleve.id) }}">
	<div style="width:900px;height:60px;margin-right:auto; margin-left:auto">
		<button class="btn btn-danger" style=" color:white;width:900px;height:60px;margin-right:auto; margin-left:auto">
			<h1>Delete</h1>
		</button>
	</div>
</form>
